//switch statement
import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner scan=new Scanner(System.in);
        int choice=scan.nextInt();
        switch(choice){
            case 1:System.out.println("In Case1");
            break;
            case 2:System.out.println("In case2");
            break;
            case 3:System.out.println("In case3");
            break;
            default:System.out.println("Invalid");
        }
    }
}
