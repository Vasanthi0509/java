import java.util.*;
class Main {
    public static void main(String[] args) {
    Scanner scan=new Scanner(System.in);
    System.out.print("Enter number:");
    int n=scan.nextInt();
        int count=1;
        for(int i=2;i<=n;i++){
            if(n%i==0){
                count=count+1;
            }
        }    
        if(count==2)
        System.out.print("number is prime");
        else
        System.out.println("number is not prime");
        
        }
}
