import java.util.*;
class Main{
    public static void main(String args[])
    {
        Scanner scan=new Scanner(System.in);
        System.out.print("Enter age: ");
        int age=scan.nextInt();
        if (age>=18)
        {
            System.out.println("Eligible for votinig");
        }
        else{
            System.out.println("Not Eligible for voting ");
        }
    }
}
