import java.util.*;
class Main{
    public static void main(String args[]){
        Scanner scan=new Scanner(System.in);
        System.out.print("Input: ");
        int num=scan.nextInt();
        int sum=0;
        for(int i=1;i<=num;i++){
            int res=num*i;
            sum=sum+res;
        }
        System.out.println("output: "+sum);
    }
}
